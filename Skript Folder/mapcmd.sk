# /map command sends a link to the server map to the player
options:
	# change the following to the link of your map
	link: http://map.serverip.com:1234/

command /map:
	trigger:
		send "&e-----------------" to player
		send "&6 Check out the map!" to player
		send "" to player
		send "&b - Live Map: <link:{@link}>&nClick Me!" to player
		send "&e-----------------" to player